{
  "name": "gmap-vue-next",
  "version": "3.5.2",
  "description": "This is a google map component for Vue.js, updated for Vue 2 compatibility",
  "main": "dist/main.js",
  "module": "dist/esm/main.js",
  "unpkg": "dist/gmap-vue.min.js",
  "jsdelivr": "dist/gmap-vue.min.js",
  "browser": {
    "./sfc": "dist/components"
  },
  "engines": {
    "node": ">=12",
    "npm": ">6"
  },
  "scripts": {
    "clean:build": "rimraf ./dist && rimraf ./build && rimraf ./types",
    "env:prod": "cross-env NODE_ENV=production",
    "env:test": "cross-env NODE_ENV=test",
    "env:dev": "cross-env NODE_ENV=development",
    "rollup": "rollup --config",
    "tsc:build": "tsc --project tsconfig.app.json",
    "build": "pnpm run clean:build && pnpm run tsc:build && pnpm run env:prod && pnpm run rollup",
    "build:test": "pnpm run clean:build && pnpm run tsc:build && pnpm run env:test && pnpm run rollup",
    "build:dev": "pnpm run clean:build && pnpm run tsc:build && pnpm run env:dev && pnpm run rollup",
    "lint": "prettier --write ./src && eslint --fix --ext .vue,.ts,.js,.html ./src",
    "test": "echo 'not implemented yet'",
    "lint-staged": "lint-staged",
    "gen:api:docs": "rimraf ../documentation/docs/code && npx vuepress-jsdoc -d ../documentation/docs -t \"GmapVue API\" -r ./README.md",
    "release": "semantic-release"
  },
  "exports": {
    "import": "./dist/esm/main.js",
    "require": "./dist/main.js",
    "default": "./dist/esm/main.js"
  },
  "files": [
    "dist"
  ],
