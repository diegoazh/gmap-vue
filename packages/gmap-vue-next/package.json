{
  "name": "gmap-vue-next",
  "version": "3.5.2",
  "description": "This is a google map component for Vue.js, updated for Vue 2 compatibility",
  "main": "dist/main.umd.js",
  "module": "dist/main.es.js",
  "typings": "dist/types/main.d.ts",
  "unpkg": "dist/main.umd.js",
  "jsdelivr": "dist/main.umd.js",
  "browser": {
    "./sfc": "dist/components"
  },
  "engines": {
    "node": ">=12",
    "npm": ">6"
  },
  "scripts": {
    "clean:build": "rimraf ./dist && rimraf ./build && rimraf ./types",
    "env:prod": "cross-env NODE_ENV=production",
    "env:test": "cross-env NODE_ENV=test",
    "env:dev": "cross-env NODE_ENV=development",
    "dev": "vite",
    "dev:e2e": "pnpm run build && vite --config cypress/runner/vite.config.js",
    "preview": "vite preview --port 4173",
    "build": "vite build",
    "tsc:build": "tsc --project tsconfig.app.json",
    "vite:build": "run-p type-check build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom",
    "coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test test:e2e:vite:server http://localhost:4173/ 'pnpx cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test test:e2e:vite:server http://localhost:4173/ 'pnpx cypress run --e2e'",
    "test:e2e:build": "vite build --config cypress/runner/vite.config.js",
    "test:e2e:preview": "vite preview --config cypress/runner/vite.config.js",
    "test:e2e:vite:server": "pnpm run test:e2e:build && pnpm run test:e2e:preview",
    "test:e2e:node:server": "node cypress/runner/runner.cjs",
    "lint": "prettier --write ./src && eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint-staged": "lint-staged",
    "copy:components": "cpx \"./src/components/*.vue\" \"./build/src/components\" --verbose",
    "release": "semantic-release"
  },
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "require": "./dist/main.umd.js",
      "default": "./dist/main.es.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/diegoazh/gmap-vue.git"
  },
  "bugs": {
    "url": "https://github.com/diegoazh/gmap-vue/issues"
  },
  "homepage": "https://github.com/diegoazh/gmap-vue#readme",
  "dependencies": {
    "@googlemaps/markerclusterer": "^2.0.13",
    "lodash.isequal": "^4.5.0",
    "mitt": "^3.0.0",
    "vue": "^3.2.44"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@swc/core": "^1.3.14",
    "@types/google.maps": "^3.50.5",
    "@types/jsdom": "^20.0.1",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.25.1",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.1",
    "@vue/tsconfig": "^0.1.3",
    "@vue/vue3-jest": "^29.2.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^11.0.1",
    "cypress-plugin-tab": "^1.0.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.7.0",
    "eslint-plugin-vuejs-accessibility": "^2.0.0",
    "gh-pages": "^4.0.0",
    "happy-dom": "^7.6.7",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "semantic-release-cli": "^5.4.4",
    "start-server-and-test": "^1.14.0",
    "stylus": "^0.59.0",
    "typescript": "~4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.0",
    "vitest": "^0.25.1",
    "vue-tsc": "^1.0.9"
  },
  "author": "Diego A. Zapata HÃ¤ntsch, Daniel Sim, Guillaume Leclerc",
  "license": "MIT"
}
