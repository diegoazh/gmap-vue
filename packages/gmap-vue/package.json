{
  "name": "gmap-vue",
  "version": "1.4.1",
  "description": "This is a google map component for Vue.js, updated for Vue 2 compatibility",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf ./dist && shx mkdir -p ./dist && cross-env BUILD_DEV=1 babel src -D --out-dir ./dist && echo {} > dist/.babelrc",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "build:dev": "npm run prebuild && cross-env NODE_ENV=development webpack --progress",
    "build:test": "npm run prebuild && cross-env NODE_ENV=test webpack --progress",
    "lint": "prettier --write ./src && eslint --fix --ext .vue,.js,.html ./src",
    "test": "echo 'not implemented yet'",
    "prepare": "npm run build",
    "lint-staged": "lint-staged",
    "gen:api:docs": "rimraf ../documentation/docs/code && npx vuepress-jsdoc -d ../documentation/docs -t \"GmapVue API\" -r ./README.md"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegoazh/gmap-vue.git"
  },
  "bugs": {
    "url": "https://github.com/diegoazh/gmap-vue/issues"
  },
  "homepage": "https://github.com/diegoazh/gmap-vue#readme",
  "dependencies": {
    "@google/markerclustererplus": "^5.1.3",
    "core-js": "^3.16.0",
    "regenerator-runtime": "^0.13.9",
    "vue-docgen-cli": "^4.40.0",
    "vue-loader": "^15.9.7"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "better-docs": "^2.3.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.15.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "jsdoc": "^3.6.7",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.21",
    "prettier": "^2.3.2",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
